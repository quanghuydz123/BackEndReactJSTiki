const bcrypt = require('bcrypt')

const createCategory = (Category)=>{
    return new Promise(async (resolve,reject)=>{
        try{
            resolve({
                status:"OK",
                message:"Thêm thành công",
                data:Category
            })
            
        }
        catch(e){
            reject(e)
        }
    })
}



module.exports = {
    createCategory,
}